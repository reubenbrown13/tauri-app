{
  "identifier": "com.tauri-clock-app.app",
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-clock-app",
  "version": "0.1.1",
    "build": {
      "beforeDevCommand": "npm run dev",
      "devUrl": "http://localhost:3000",
      "beforeBuildCommand": "npm run build",
      "frontendDist": "../dist"
    },
    "app": {
      "windows": [
        {
          "title": "tauri-clock-app",
          "width": 1150,
          "height": 775
        }
      ],
      "security": {
        "csp": null
      }
    },
    "bundle": {
      "resources": ["assets/alarm-default.mp3"],
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
  
